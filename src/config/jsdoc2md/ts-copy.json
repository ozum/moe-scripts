{
  "source": {
    "includePattern": ".+\\.(j|t)s(doc|x)?$",
    "excludePattern": "(.+\\.(test|spec).ts)|__tests__|__mocks__|__test_supplements__|__test_helpers__"
  },
  "plugins": [
    "plugins/markdown",
    "node_modules/moe-scripts/node_modules/jsdoc-babel"
  ],
  "babel": {
    "extensions": ["ts", "tsx"],
    "ignore": ["**/*.(test|spec).ts"],
    "babelrc": false,
    "presets": [["./node_modules/moe-scripts/node_modules/@babel/preset-env", { "targets": { "node": "current" } }], "./node_modules/moe-scripts/node_modules/@babel/preset-typescript"],
    "plugins": ["./node_modules/moe-scripts/node_modules/@babel/plugin-proposal-class-properties", "./node_modules/moe-scripts/node_modules/@babel/plugin-proposal-object-rest-spread"]
  }
}
