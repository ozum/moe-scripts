{
  "name": "moe-scripts",
  "version": "0.0.46",
  "description": "CLI for common scripts for my projects",
  "engines": {
    "node": ">= 8",
    "npm": ">= 5"
  },
  "bin": {
    "moe-scripts": "dist/index.js"
  },
  "scripts": {
    "preinstall": "if [ ! -e src ]; then node dist init-pre; fi",
    "postinstall": "if [ ! -e src ]; then node dist init; fi",
    "watch:build": "node src build --source-maps --copy-files --out-dir dist --watch",
    "watch:test": "node src test --watch",
    "test": "node src test",
    "test:update": "node src test --updateSnapshot",
    "build": "node src build --source-maps --copy-files --out-dir dist",
    "lint": "node src lint",
    "format": "node src format",
    "validate": "node src validate",
    "postversion": "git push && git push --tags && npm publish",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "babel.js",
    "eslint.js",
    "config.js",
    "prettier.js",
    "jest.js",
    "husky.js",
    "commitlint.js",
    "tslint-backend.json"
  ],
  "keywords": [],
  "author": "Mehmet Ozum Eldogan",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.0.0-beta.42",
    "@babel/core": "^7.0.0-beta.42",
    "@babel/plugin-external-helpers": "^7.0.0-beta.42",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.42",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.42",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0-beta.42",
    "@babel/preset-env": "^7.0.0-beta.42",
    "@babel/preset-react": "^7.0.0-beta.42",
    "@babel/preset-typescript": "^7.0.0-beta.42",
    "@commitlint/cli": "^6.1.3",
    "@commitlint/config-conventional": "^6.1.3",
    "@types/jest": "^22.2.0",
    "@types/node": "^9.4.7",
    "ajv": "^6.3.0",
    "all-contributors-cli": "^4.11.1",
    "arrify": "^1.0.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^22.0.4",
    "babel-macros": "^1.2.0",
    "babel-plugin-minify-dead-code-elimination": "^0.3.0",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-inline-environment-variables": "^0.3.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "chalk": "^2.3.2",
    "commitizen": "^2.9.6",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.4",
    "cross-spawn": "^6.0.5",
    "cz-conventional-changelog": "^2.1.0",
    "doctoc": "^1.3.0",
    "eslint": "^4.19.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-security": "^1.4.0",
    "fs-extra": "^5.0.0",
    "glob": "^7.1.2",
    "handlebars": "^4.0.11",
    "husky": "^0.15.0-rc.12",
    "is-ci": "^1.1.0",
    "jest": "^22.4.2",
    "jsdoc-babel": "^0.4.0",
    "jsdoc-to-markdown": "^4.0.1",
    "lint-staged": "^7.0.0",
    "load-json-file": "^4.0.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.has": "^4.5.2",
    "lodash.omit": "^4.5.0",
    "lodash.set": "^4.3.2",
    "mkdirp": "^0.5.1",
    "nodemon": "^1.17.2",
    "nsp": "^3.2.1",
    "prettier": "^1.11.1",
    "read-pkg-up": "^3.0.0",
    "resolve": "^1.5.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.2.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0",
    "sort-keys": "^2.0.0",
    "source-map-support": "^0.5.4",
    "standard-version": "^4.3.0",
    "ts-jest": "^22.4.2",
    "ts-node": "^5.0.1",
    "tsc-watch": "^1.0.17",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.7.0",
    "tslint-config-prettier": "^1.10.0",
    "typescript": "^2.7.2",
    "which": "^1.3.0",
    "write-json-file": "^2.3.0",
    "yargs-parser": "^9.0.2"
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "rules": {
      "no-process-exit": "off",
      "import/no-dynamic-require": "off",
      "import/no-unassigned-import": "off",
      "no-console": "off",
      "no-nested-ternary": "off"
    }
  },
  "eslintIgnore": [
    "node_modules",
    "coverage",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "ozum/moe-scripts"
  },
  "bugs": {
    "url": "https://github.com/ozum/moe-scripts/issues"
  },
  "devDependencies": {
    "jest-in-case": "^1.0.2",
    "slash": "^1.0.0"
  }
}
