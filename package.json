{
  "name": "moe-scripts",
  "version": "0.0.2",
  "description": "CLI for common scripts for my projects",
  "engines": {
    "node": ">= 8",
    "npm": ">= 5"
  },
  "bin": {
    "moe-scripts": "dist/index.js"
  },
  "scripts": {
    "install": "cd ../.. && node_modules/.bin/moe-scripts init",
    "test": "node src test --watchAll",
    "test:update": "node src test --updateSnapshot",
    "build": "node src build --watch --source-maps --copy-files --out-dir dist",
    "lint": "node src lint",
    "format": "node src format",
    "validate": "node src validate",
    "postversion": "git push && git push --tags && npm publish",
    "prepublishOnly": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "node src precommit"
    }
  },
  "files": [
    "dist",
    "babel.js",
    "eslint.js",
    "config.js",
    "prettier.js",
    "jest.js",
    "husky.js",
    "tslint-backend.json"
  ],
  "keywords": [],
  "author": "Kent C. Dodds <kent@doddsfamily.us> (http://kentcdodds.com/)",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/plugin-external-helpers": "^7.0.0-beta.40",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.40",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.40",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@babel/preset-react": "^7.0.0-beta.40",
    "@babel/preset-typescript": "^7.0.0-beta.40",
    "@types/jest": "^22.1.4",
    "@types/node": "^9.4.6",
    "ajv": "^6.2.1",
    "all-contributors-cli": "^4.10.1",
    "arrify": "^1.0.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^22.0.4",
    "babel-macros": "^1.2.0",
    "babel-plugin-minify-dead-code-elimination": "^0.3.0",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-inline-environment-variables": "^0.3.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "chalk": "^2.3.2",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.3",
    "cross-spawn": "^6.0.5",
    "doctoc": "^1.3.0",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.12.3",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-security": "^1.4.0",
    "glob": "^7.1.2",
    "handlebars": "^4.0.11",
    "husky": "^0.15.0-rc.8",
    "is-ci": "^1.1.0",
    "jest": "^22.4.2",
    "jsdoc-babel": "^0.4.0-alpha.0",
    "jsdoc-to-markdown": "^4.0.1",
    "lint-staged": "^7.0.0",
    "load-json-file": "^4.0.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.has": "^4.5.2",
    "lodash.omit": "^4.5.0",
    "lodash.set": "^4.3.2",
    "mkdirp": "^0.5.1",
    "nsp": "^3.2.1",
    "prettier": "^1.11.1",
    "read-pkg-up": "^3.0.0",
    "resolve": "^1.5.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.56.3",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0",
    "source-map-support": "^0.5.3",
    "ts-jest": "^22.4.1",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.7.0",
    "tslint-config-prettier": "^1.9.0",
    "typescript": "^2.7.2",
    "which": "^1.3.0",
    "write-json-file": "^2.3.0",
    "yargs-parser": "^9.0.2"
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "rules": {
      "no-process-exit": "off",
      "import/no-dynamic-require": "off",
      "import/no-unassigned-import": "off",
      "no-console": "off",
      "no-nested-ternary": "off"
    }
  },
  "eslintIgnore": [
    "node_modules",
    "coverage",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kentcdodds/moe-scripts.git"
  },
  "bugs": {
    "url": "https://github.com/kentcdodds/moe-scripts/issues"
  },
  "homepage": "https://github.com/kentcdodds/moe-scripts#readme",
  "devDependencies": {
    "jest-in-case": "^1.0.2",
    "slash": "^1.0.0"
  }
}
